{
  "name": "@soundcore/api",
  "version": "0.0.1",
  "description": "Soundcore API service written using NestJS",
  "author": "z3ttee",
  "private": true,
  "license": "MIT",
  "scripts": {
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:prod": "node dist/main",
    "prebuild": "rimraf dist",
    "build": "nest build",
    "build:docker": "docker builder prune --all --force && docker build -t soundcore:latest .",
    "bundle": "npm run build && node bundle/bundle-packagejson.js && node bundle/bundle-env.js && node bundle/bundle-info.js",
    "bundle:pm2": "npm run bundle && npm run bundle/bundle-pm2.js ",
    "bundle:docker": "npm run bundle && docker builder prune --all --force && docker build -t z3ttee/soundcore:latest .",
    "docker-compose": "docker-compose up",
    "depcheck": "npx depcheck"
  },
  "dependencies": {
    "@nestjs/bull": "^0.5.5",
    "@nestjs/common": "^8.4.6",
    "@nestjs/config": "^2.0.1",
    "@nestjs/core": "^8.4.6",
    "@nestjs/event-emitter": "^1.1.1",
    "@nestjs/jwt": "^8.0.1",
    "@nestjs/mapped-types": "^1.0.1",
    "@nestjs/platform-express": "^8.4.6",
    "@nestjs/platform-socket.io": "^8.4.6",
    "@nestjs/schedule": "^2.0.1",
    "@nestjs/typeorm": "^8.1.2",
    "@nestjs/websockets": "^8.4.6",
    "@tsalliance/rest": "^2.0.75",
    "@tsalliance/utilities": "^1.18",
    "axios": "^0.24.0",
    "bull": "^4.6.2",
    "class-transformer": "^0.4.0",
    "class-validator": "^0.13.2",
    "ffmpeg-static": "^4.4.0",
