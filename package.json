    "@types/ffprobe": "^1.1.2",
    "@types/glob": "^7.2.0",
    "@types/multer": "^1.4.7",
    "@types/node": "^16.0.0",
    "@types/sharp": "^0.29.4",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^8.0.1",
    "source-map-support": "^0.5.20",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
