    "ffprobe-static": "^3.0.0",
    "glob": "^7.2.0",
    "mysql2": "^2.3.3",
    "nestjs-pager": "^1.0.25",
    "node-id3": "^0.2.3",
    "node-vibrant": "^3.2.1-alpha.1",
    "openid-client": "^5.1.5",
    "redlock": "^5.0.0-beta.2",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.2.0",
    "sanitize-filename": "^1.6.3",
    "sharp": "^0.30.6",
